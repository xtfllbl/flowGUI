<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE ui>
<ui version="1.0">
<module name="FirstBreakAutoPick" version="1.0">

<property name="jdtpath">
	<desc>program path</desc>
	<datatype>string</datatype>
	<displaytype name="lineedit"></displaytype>
</property>

<property name="minvelo">
	<desc>minimum V</desc>
<datatype>int</datatype>
<displaytype name="lineedit">1500</displaytype>
</property>

<property name="maxvel">
	<desc>maximum V</desc>
<datatype>int</datatype>
<displaytype name="lineedit">2000</displaytype>
</property>

<property name="tw">
	<desc>time window width</desc>
	<datatype>int</datatype>
	<displaytype name="lineedit">40</displaytype>
</property>

<property name="smooth">
	<desc>should be smooth?</desc>
	<displaytype name="radiobutton">
		<option value="NO">1</option>
		<option value="YES">0</option>
	</displaytype>
</property>

<property name="pickFileName">
	<desc>pick file path and name </desc>
	<datatype>string</datatype>
	<displaytype name="lineedit">/home/lub/program/qt/flow/pick1.pick</displaytype>
</property>
</module>
</ui>

